{
    "version": 2,
    "cleanUrls": true,
    "builds": [
        {
            "src": "index.html",
            "use": "@vercel/static"
        },
        {
            "src": "search.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/search(?<query>.+)",
            "dest": "/search.js?q=$query"
        }
    ]
}